<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.farmstudio">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/sq"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/sq"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Cities"></activity>
        <activity android:name=".Weather" />
        <activity android:name=".index" />
        <activity android:name=".States" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>